{
  "name": "nextjs-github-browser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:storybook": "GITHUB_GRAPHQL_API_TOKEN=null STORYBOOK_DO_NOT_CALL_GITHUB_DIRECTLY_WITH_TOKEN=true build-storybook -o ./public/explore/storybook",
    "build:all": "npm run build:storybook && npm run build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "lint": "eslint --ext js,jsx,ts,tsx --no-fix .",
    "lint:fix": "eslint --ext js,jsx,ts,tsx --fix .",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "graphql-codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "deepmerge": "^4.2.2",
    "graphql": "^15.5.0",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@graphql-codegen/cli": "1.21.4",
    "@octokit/graphql-schema": "^10.39.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.9",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "msw": "^0.28.2",
    "postcss": "^8.2.13",
    "prettier": "2.2.1",
    "storybook-addon-themes": "^6.1.0",
    "storybook-css-modules-preset": "^1.1.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-document-nodes": "1.17.11",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
